<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="almanakka.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="almanakka.app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="almanakka.app.NormalSelectionActivity"
            android:label="NormalSelection" />
        <activity
            android:name="almanakka.app.TapRangeSelectionActivity"
            android:label="TapRangeSelection" />
        <activity
            android:name="almanakka.app.SlideRangeSelectionActivity"
            android:label="SlideRangeSelection" />
        <activity
            android:name="almanakka.app.PagerActivity"
            android:label="Pager" />
        <activity
            android:name="almanakka.app.StickyScrollActivity"
            android:label="StickyScroll" />
        <activity
            android:name="almanakka.app.StickyPagerActivity"
            android:label="StickyPager" />
        <activity
            android:name="almanakka.app.DynamicSettingActivity"
            android:label="DynamicSetting" />
    </application>

</manifest>